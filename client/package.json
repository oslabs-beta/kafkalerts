    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "babel-plugin-macros": "^3.1.0",
    "chart.js": "^4.3.0",
    "cors": "^2.8.5",
    "react": "^18.2.0",
    "react-aria": "^3.24.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "react-scroll": "^1.8.9",
