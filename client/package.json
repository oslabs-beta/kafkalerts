    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^29.5.0",
    "jest-axe": "^7.0.1",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.28",
    "postcss-cli": "^10.1.0",
